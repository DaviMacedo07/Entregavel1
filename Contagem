function contarNumerosNoIntervalo(arr, n) {
    let contador = 0;
    for (let i = 0; i < arr.length; i++) {
        if (arr[i] >= arr[0] && arr[i] <= n) {
            contador++;
        }
    }
    return contador;
}

const dados = [3, 5, 7, 2, 8, 10, 1];
const n = 6;
console.log(`Existem ${contarNumerosNoIntervalo(dados, n)} nÃºmeros no intervalo entre o primeiro dado e ${n}.`);
